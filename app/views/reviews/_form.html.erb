<%= form_for(@review, url: movie_reviews_url(@movie))  do |f| %>
	<div id="rating-form">
		<label>Rating</label>
	</div>
	<%= f.label :comment %>
  <%= f.text_field :comment %>
	<%= f.button :submit%>
<% end %>

<script>
	$('#rating-form').raty({
		path: '/assets/',
		scoreName: 'review[rating]'
	});
</script>
